<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="VB_Holder" alignment="CENTER" style="-fx-background-color: aqua;" stylesheets="/application/CSS/slogin.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controllers.AdminPage1Controller">
   <children>
      <Label alignment="CENTER" prefHeight="16.0" prefWidth="706.0" styleClass="heading" stylesheets="/application/CSS/buttonIm.css" text="Admin Login">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </Label>
      <HBox alignment="CENTER" prefHeight="30.0" prefWidth="600.0">
         <children>
            <Label fx:id="L_greet" prefHeight="24.0" prefWidth="456.0" styleClass="greet" stylesheets="/application/CSS/buttonIm.css" text="Welcome">
               <HBox.margin>
                  <Insets left="5.0" right="20.0" />
               </HBox.margin>
            </Label>
            <Button id="Bt_Logout" fx:id="Bt_Logout" mnemonicParsing="false" onAction="#f_logout" prefHeight="27.0" prefWidth="116.0" text="Logout">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER" spacing="50.0">
         <children>
            <Button fx:id="Bt_RespondToReq" contentDisplay="TOP" mnemonicParsing="false" onAction="#f_respondToRequests" styleClass="request" stylesheets="/application/CSS/buttonIm.css" text="Respond to Requests" />
            <Button fx:id="Bt_ViewBooking" contentDisplay="TOP" mnemonicParsing="false" onAction="#f_viewBookings" styleClass="viewBookings" stylesheets="/application/CSS/buttonIm.css" text="View Room Bookings" />
            <Button fx:id="Bt_ViewTimeTable" contentDisplay="TOP" mnemonicParsing="false" onAction="#f_viewTimeTable" styleClass="timetable" stylesheets="/application/CSS/buttonIm.css" text="View Time Table" />
            <Button fx:id="Bt_BookRoom" contentDisplay="TOP" mnemonicParsing="false" onAction="#f_bookRoom" styleClass="room" stylesheets="/application/CSS/buttonIm.css" text="Book a Room" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="30.0" top="20.0" />
   </padding>
</VBox>
