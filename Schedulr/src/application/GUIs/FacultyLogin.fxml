<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" style="-fx-background-color: aqua;" stylesheets="@../CSS/slogin.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controllers.FacultyLoginController">
   <children>
      <Label alignment="CENTER" prefHeight="16.0" prefWidth="657.0" styleClass="heading" stylesheets="@../CSS/buttonIm.css" text="Faculty Login">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </Label>
      <VBox>
         <children>
            <HBox alignment="CENTER">
               <children>
                  <Label fx:id="L_greet" contentDisplay="CENTER" styleClass="greet" stylesheets="@../CSS/buttonIm.css" text="Weclome " textAlignment="CENTER">
                     <HBox.margin>
                        <Insets left="5.0" right="250.0" />
                     </HBox.margin>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </Label>
                  <Button id="Bt_Logout" fx:id="Bt_logout" mnemonicParsing="false" onAction="#f_logout" prefHeight="27.0" prefWidth="76.0" text="Logout">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets bottom="20.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </HBox>
            <ButtonBar>
               <buttons>
                  <Button fx:id="Bt_viewRoomBookings" contentDisplay="TOP" mnemonicParsing="false" onAction="#f_viewRoomBookings" styleClass="viewBookings" stylesheets="@../CSS/buttonIm.css" text="View Room Bookings" />
                  <Button fx:id="Bt_viewLabel" contentDisplay="TOP" mnemonicParsing="false" onAction="#f_viewTimeTable" styleClass="timetable" stylesheets="@../CSS/buttonIm.css" text="View Time Table" />
                  <Button fx:id="Bt_viewCourse" contentDisplay="TOP" mnemonicParsing="false" onAction="#f_cancelBooking" styleClass="course" stylesheets="@../CSS/buttonIm.css" text="View Courses" />
                  <Button fx:id="Bt_bookRoom" contentDisplay="TOP" mnemonicParsing="false" onAction="#f_bookRoom" styleClass="room" stylesheets="@../CSS/buttonIm.css" text="Book a Room" />
               </buttons>
            </ButtonBar>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="30.0" top="20.0" />
   </padding>
</VBox>
