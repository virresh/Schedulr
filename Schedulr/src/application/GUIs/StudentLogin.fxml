<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controllers.StudentLoginController">
   <children>
      <VBox>
         <children>
            <Label alignment="CENTER" prefHeight="16.0" prefWidth="622.0" text="Student Login">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Label>
            <HBox>
               <children>
                  <Label fx:id="L_greet" prefHeight="26.0" prefWidth="517.0" text="Welcome">
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </Label>
                  <Button id="Bt_Logout" fx:id="Bt_logout" mnemonicParsing="false" onAction="#f_logout" prefHeight="27.0" prefWidth="76.0" text="Logout" />
               </children>
            </HBox>
         </children>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </VBox>
      <FlowPane alignment="CENTER" hgap="20.0" vgap="20.0">
         <children>
            <Button fx:id="Bt_viewCourses" mnemonicParsing="false" onAction="#f_viewCourses" prefHeight="113.0" prefWidth="171.0" text="View Courses">
               <padding>
                  <Insets left="15.0" />
               </padding>
            </Button>
            <Button fx:id="Bt_bookRoom" mnemonicParsing="false" onAction="#f_bookRoom" prefHeight="100.0" prefWidth="135.0" text="Book a Room" />
            <Button fx:id="Bt_viewTimeTable" mnemonicParsing="false" onAction="#f_viewTimeTable" prefHeight="100.0" prefWidth="165.0" text="View Time Table" />
            <Button fx:id="Bt_viewRoomBookings" mnemonicParsing="false" onAction="#f_viewRoomBookings" prefHeight="99.0" prefWidth="171.0" text="View Room Bookings">
               <padding>
                  <Insets left="15.0" />
               </padding>
            </Button>
         </children>
      </FlowPane>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
