<ScrollPane fitToHeight="true" fitToWidth="true" style="-fx-background-color: aqua;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controllers.ViewTimeTable_StudentController">
   <content>
      <FlowPane alignment="CENTER" columnHalignment="CENTER" style="-fx-background-color: aqua;">
         <children>
            <VBox fx:id="V_backBox" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" stylesheets="/application/CSS/slogin.css">
               <children>
                  <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
                     <children>
                        <Button fx:id="Bt_back" mnemonicParsing="false" onAction="#f_back" text="&lt;" />
                        <FlowPane alignment="CENTER" columnHalignment="CENTER">
                           <children>
                              <Label alignment="CENTER" styleClass="heading" stylesheets="/application/CSS/buttonIm.css" text="View TimeTable">
                                 <padding>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </padding>
                              </Label>
                           </children>
                        </FlowPane>
                        <ComboBox fx:id="CB_select" onAction="#f_updatedTT" promptText="Select" />
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </VBox>
         </children>
      </FlowPane>
   </content>
